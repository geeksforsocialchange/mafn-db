<%= semantic_form_for(@member_location) do |f| %>
  <%= f.inputs do %>
    <%= f.input :member, :collection => Member.order(:last_name) %>
    <%= f.input :location, :collection => Location.order(:name) %>
    <%= f.input :from, :as => :date_picker %>
    <%= f.input :to, :as => :date_picker %>
  <% end %>

  <%= f.actions do %>
    <%= f.submit %>
  <% end %>
<% end %>
