<%= semantic_form_for(@member) do |f| %>

  <%= f.inputs do %>
    <%= f.input :first_name %><br>
    <%= f.input :last_name %><br>
    <%= f.input :dob %>
    <%= f.input :gender %>
    <%= f.input :tel %>
    <%= f.input :mob %>
    <%= f.input :email %>
  <% end %>
  <%= f.actions do %>
     <%= f.action :submit, :as => :button %>
     <%= f.action :cancel, :as => :link %>
   <% end %>
<% end %>
