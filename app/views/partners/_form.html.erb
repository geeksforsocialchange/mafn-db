<%= semantic_form_for @partner do |f| %>
  <%= f.inputs do %>
    <%= f.input :start %>
    <%= f.input :finish %>
    <%= f.input :type %>
    <%= f.input :organisation %>
    <%= f.input :project %>
  <% end %>

  <%= f.actions do %>
    <%= f.action :submit, :as => :input %>
  <% end %>
<% end %>
