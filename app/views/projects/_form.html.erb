<%= semantic_form_for @project do |f| %>
  <%= f.inputs do %>
    <%= f.input :name %>
    <%= f.input :start %>
    <%= f.input :finish %>
    <%= f.input :primary_partner_organisation %>
    <%= f.input :resident_champion %>
    <%= f.input :resident_seconder %>
  <% end %>

  <%= f.actions do %>
    <%= f.action :submit, :as => :input %>
  <% end %>
<% end %>
