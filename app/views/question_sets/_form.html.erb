<%= semantic_form_for(@question_set) do |f| %>
  <%= f.inputs do %>
    <%= f.input :title %>
  <% end %>
  <div id="events">
    <%= f.semantic_fields_for :questions do |question| %>
      <%= render 'question_fields', f: question %>
    <% end %>
    <div class="links">
      <%= link_to_add_association "Add Question", f, :questions %>
    </div>
    <%= f.actions do %>
      <%= f.action :submit, :as => :button %>
    <% end %>
  </div>
<% end %>
