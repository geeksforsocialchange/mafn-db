<%= semantic_form_for @volunteer do |f| %>
  <%= f.inputs do %>
    <%= f.input :role %>
    <%= f.input :is_leader %>
    <%= f.input :start %>
    <%= f.input :finish %>
    <%= f.input :member %>
    <%= f.input :project %>
  <% end %>

  <%= f.actions do %>
    <%= f.action :submit, :as => :input %>
  <% end %>
<% end %>
